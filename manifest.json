{
  "name": "Slack plugin",
  "version": "1.0",
  "manifest_version": 2,
  "description": "Build an Extension!",
  "permissions": ["webRequest", "activeTab", "declarativeContent", "storage", "tabs"],
  "browser_action": {
    "default_popup": "popup/index.html",
    "default_icon": {
      "16": "images/get_started16.png",
      "32": "images/get_started32.png",
      "48": "images/get_started48.png",
      "128": "images/get_started128.png"
    }
  },
  "commands": {
    "Ctrl+B": {
      "suggested_key": {
        "default": "Ctrl+B",
        "mac": "Command+B"
      },
      "description": "Ctrl+B."
    }
  },
  "icons": {
    "16": "images/get_started16.png",
    "32": "images/get_started32.png",
    "48": "images/get_started48.png",
    "128": "images/get_started128.png"
  }
}
